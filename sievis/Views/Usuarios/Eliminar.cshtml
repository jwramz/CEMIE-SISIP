@model sievis.Models.AppUsers

<!-- modal -->
@using (Html.BeginForm("Eliminar", "Usuarios", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    <div class="bootstrap-dialog.type-primary modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">¿Esta seguro que desea eliminar al Usuario?</h4>
    </div>
    <div class="modal-body">
        <div class="form-horizontal">
            <dl class="dl-horizontal">
                <dt>AppUserId</dt>
                <dd>
                    @Html.DisplayFor(model => model.AppUserId)
                </dd>
                <dt>Estatus</dt>
                <dd>
                    @Html.DisplayFor(model => model.Estatus)
                </dd>
                <dt>Nombre Completo</dt>
                <dd>
                    @Html.DisplayFor(model => model.NombreCompleto)
                </dd>
                <dt>Email</dt>
                <dd>
                    @Html.DisplayFor(model => model.AspNetUsers.Email)
                </dd>
                <dt>Role</dt>
                <dd>
                    @Html.DisplayFor(model => model.AspNetRoles.Name)
                </dd>
                <dt>Gerencia</dt>
                <dd>
                    @Html.DisplayFor(model => model.Gerencia.nombre)
                </dd>
                <dt>Zona</dt>
                <dd>
                    @Html.DisplayFor(model => model.Zona.nombre)
                </dd>
                <dt>Subestacion</dt>
                <dd>
                    @Html.DisplayFor(model => model.Subestacion.nombre)
                </dd>
            </dl>
        </div>
    </div>
    <div class="modal-footer">
        <button type="submit" class="btn btn-primary" id="modalBtnSave">Aceptar</button>
        <button type="button" class="btn btn-info" id="modalBtnCancel" data-dismiss="modal">Cancelar</button>
    </div>
}

